health_costs <- wb(country = high_income_countries,
indicator = c("SH.XPD.CHEX.PC.CD","SH.XPD.GHED.PC.CD","SH.XPD.PVTD.PC.CD","SH.XPD.OOPC.PC.CD"),
mrv = 1,
cache = update_cache) %>%
arrange(-value)
health_costs[health_costs[,4] == "SH.XPD.CHEX.PC.CD", 4] <- "Total Spending"
health_costs[health_costs[,4] == "SH.XPD.GHED.PC.CD", 4] <- "Government Spending"
health_costs[health_costs[,4] == "SH.XPD.PVTD.PC.CD", 4] <- "Private Spending"
health_costs[health_costs[,4] == "SH.XPD.OOPC.PC.CD", 4] <- "Out of Pocket Costs"
colnames(health_costs)[4] <- "Type"
total_health_costs <- health_costs %>% filter(Type == "Total Spending")
